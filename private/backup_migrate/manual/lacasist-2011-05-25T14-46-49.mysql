CREATE TABLE `comment_notify` (   `cid` int(10) unsigned NOT NULL COMMENT 'The comment id from comments.cid',   `notify` tinyint(4) NOT NULL COMMENT 'A boolean indicator for whether or not they subscribed: 1 means subscribed, 0 means not subscribed.',   `notify_hash` varchar(128) NOT NULL DEFAULT '' COMMENT 'A hash of unique information about the commenter.  Used for unsubscribing users.',   `notified` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'A boolean indicator for whether or not a notification for the comment has been sent: 1 means yes, 0 means no.',   PRIMARY KEY (`cid`),   KEY `notify_hash` (`notify_hash`) );
