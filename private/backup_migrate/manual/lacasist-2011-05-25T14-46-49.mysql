DROP TABLE IF EXISTS `advanced_help_index`;
CREATE TABLE `advanced_help_index` (   `sid` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'The primary key to give to the search engine for this topic.',   `module` varchar(255) NOT NULL DEFAULT '' COMMENT 'The module that owns this topic.',   `topic` varchar(255) NOT NULL DEFAULT '' COMMENT 'The topic id.',   `language` varchar(12) NOT NULL DEFAULT '' COMMENT 'The language this search index relates to.',   PRIMARY KEY (`sid`),   KEY `language` (`language`) ) AUTO_INCREMENT=1;
DROP TABLE IF EXISTS `aggregator_category`;
